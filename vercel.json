{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "**/*.html", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" },
    { "src": "/*.js", "use": "@vercel/static" },
    { "src": "/*.jpg", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*\\.html)$", "dest": "/$1" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg))$", "dest": "/$1" }
  ]
}
